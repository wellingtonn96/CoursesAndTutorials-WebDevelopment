<!DOCTYPE html>
<html lang="pt-br">
<head>
    <title>Document</title>
</head>
<body>
    <table>
        <tr>
            <td>Id</td>
            <td>Titulo</td>
            <td>Noticia</td>
        </tr>
        <% for(var i=0; i < noticias.length; i++) {%>
            <tr>
                <td><%= noticias[i].id_noticias %></td>
                <td><%= noticias[i].titulo  %></td>
                <td><%= noticias[i].noticia  %></td>
            </tr>
        <% } %>
    </table>
</body>
</html>